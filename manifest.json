{
    "manifest_version": 2,
    "name": "Palmers Moodle Improvement Suite [PMIS]",
    "version": "1.1.4",
    "permissions": [
        "storage"
    ],
    "options_ui": {
        "page": "options.html",
        "open_in_tab": true
    },
    "description": "An extension that changes the way the USP College moodle website behaves.",
    "content_scripts": [{
        "js": [
            "videoRemove.js"
        ],
        "css": [
            "style.css"
        ],
        "matches": [
            "https://portal.uspcollege.ac.uk/moodle/*"
        ]
    }]
}